<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>
	<classPathEntry location="c:\mysql-connector-java-5.1.26.jar" />

	<context id="DB2Tables" targetRuntime="MyBatis3">

		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.16.1.207:3307/appserver" userId="appserver"
			password="cst2014">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.cst.authorize.dao.model"
			targetProject="authorize-dao">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.cst.authorize.dao.mapper.sqlmap"
			targetProject="authorize-dao\src\main\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.cst.authorize.dao.mapper" targetProject="authorize-dao">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="app_user" domainObjectName="User"></table>
		<table tableName="app_password" domainObjectName="Password"></table>
		<table tableName="app_login" domainObjectName="Login"></table>
		<table tableName="app_verify_mobile" domainObjectName="VerifyMobile"></table>
		<table tableName="app_sms_log" domainObjectName="SmsLog"></table>

	</context>

</generatorConfiguration>

